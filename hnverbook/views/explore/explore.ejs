<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <% if (theme === 'dark') { %>
    <link rel="stylesheet" href="/stylesheets/dark.css">
 <% } else { %>
    <link rel="stylesheet" href="/stylesheets/light.css">
 <% } %>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/explore/index.css' />
    <%- include('../partials/head') %>
   <script src="/scripts/index.js" defer type="module"></script>
</head>
 <body>
    
<%- include('../partials/header', {
theme, title: 'EXPLORE', leftIcon: null, click: null, titleClick: 'javascript:void(0)', rightButtons: [
   { icon: 'Filter 2', url: 'javascript:void(0)', click: 'window.history.go(-1)', size: 'normal'}
]}) %>
 
   <div id="app" class="app">
      <div id="screen" class="screen">
         <div class="sticky-layout-top">
            <div onclick="window.location.href = '/explore/search'" class="searchbar">
               <img src="/icon/stroke/Search/<%= theme === 'light' ? '171b20' : 'dbe1e8' %>/30/1.5" width="30" height="30" alt="search">
               <p>Search Accounts</p>
            </div>
         </div>
      </div>
      <%- include('../partials/navigation', {theme, active: 'explore'}) %>
   </div>
   
  </body>
</html>
